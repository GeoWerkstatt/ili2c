INTERLIS 2.4;

MODEL ModelA AT "http://www.interlis.ch/ili2c/tests/" VERSION "1" =

  DOMAIN
    CoordA = COORD 0 .. 100, 0 .. 100;

    ConcreteCoordA = COORD 0 .. 100, 0 .. 100;

  CONTEXT default =
    CoordA = ConcreteCoordA;

  TOPIC TopicA =
    CLASS ClassA =
      attrA : CoordA;
    END ClassA;
  END TopicA;

END ModelA.
